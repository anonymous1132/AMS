<mui:ModernWindow x:Class="AMS.CIM.Caojin.SqlReplicationAnalysisTool.CaojinMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        mc:Ignorable="d"  ContentSource="/Pages/RunStatus.xaml"
        Title="CaojinMainWindow" Height="600" Width="800">
    <mui:ModernWindow.TitleLinks>
        <mui:Link DisplayName="设置" Source="/Pages/Settings.xaml" />
    </mui:ModernWindow.TitleLinks>

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="AMS Sql Replication Analysis Tool">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="运行分析" Source="/Pages/RunStatus.xaml" />
                <mui:Link DisplayName="进程分析" Source="/Pages/ProgramCheck.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>

        
        <mui:LinkGroup DisplayName="设置" GroupKey="settings">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="软件设置" Source="/Pages/Settings.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>

    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
